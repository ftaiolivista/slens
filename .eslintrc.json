{
    "extends": "standard",
    "parserOptions": {
        "ecmaVersion": 2019,
        "sourceType": "module"
    },
    "rules": {
        "indent": [2, 4],
        "camelcase": [
            0,
            {
                "properties": "always"
            }
        ],
        "arrow-parens": [2, "as-needed"]
    }
}
